{
  "$schema": "./appsettings.schema.json",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AppConfig": {
    "ConnectionString": "Data Source=/appdata/Threax.Home.db;",
    "DetailedErrors": false,
    "AllowApiExplorer": false,
    "UseAssetBundles": false
  },
  "ClientConfig": {
    "ServiceUrl": "https://home.dev.threax.com/api",
    "UserDirectoryUrl": "https://localhost:44392/"
  },
  "Cors": {
    "UnlimitedAccess": true
  },
  "JwtAuth": {
    "Authority": "https://id.dev.threax.com/"
  },
  "ZWave": {
    "ComPort": "COM3",
    "Enabled": false
  }
}
