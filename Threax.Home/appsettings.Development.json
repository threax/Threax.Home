{
  "$schema": "./appsettings.schema.json",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AppConfig": {
    "ConnectionString": "Data Source=../../DevData/home/Threax.Home.db;",
    "DetailedErrors": true,
    "AllowApiExplorer": true,
    "UseAssetBundles": false,
    "AddUserSecrets": true,
    "CacheToken": "nocache"
  },
  "ClientConfig": {
    "ServiceUrl": "https://localhost:44357/api"
  },
  "JwtAuth": {
    "Authority": "https://localhost:44390"
  },
  "IdServerClient": {
    "ServiceUrl": "https://localhost:44390/api"
  },
  "SharedClientCredentials": {
    "IdServerHost": "https://localhost:44390"
  },
  "CertManager": {
    "LoadCerts": false
  },
  "LibCec": {
    "Enabled": true,
    "Port": "COM5"
  }
}
